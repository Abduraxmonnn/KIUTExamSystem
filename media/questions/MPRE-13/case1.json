[
    {
        "id": 111,
        "question": [
            {
                "type": "text",
                "content": "Til ijtimoiy hodisa.\n"
            }
        ]
    },
    {
        "id": 112,
        "question": [
            {
                "type": "text",
                "content": "Adabiy til va milliy til nima?\n"
            }
        ]
    },
    {
        "id": 113,
        "question": [
            {
                "type": "text",
                "content": "So’z yasovchi qo’shimchalar.\n"
            }
        ]
    },
    {
        "id": 114,
        "question": [
            {
                "type": "text",
                "content": "Otning egalik kategoriyasi.\n"
            }
        ]
    },
    {
        "id": 115,
        "question": [
            {
                "type": "text",
                "content": "Gap va uning turlari.\n"
            }
        ]
    },
    {
        "id": 116,
        "question": [
            {
                "type": "text",
                "content": "Til va nutq.\n"
            }
        ]
    },
    {
        "id": 117,
        "question": [
            {
                "type": "text",
                "content": "Dunyo tillari haqida umumiy ma’lumot.\n"
            }
        ]
    },
    {
        "id": 118,
        "question": [
            {
                "type": "text",
                "content": "Unli tovushlar tasnifi.\n"
            }
        ]
    },
    {
        "id": 119,
        "question": [
            {
                "type": "text",
                "content": "Otning kelishik kategoriyasi.\n"
            }
        ]
    },
    {
        "id": 120,
        "question": [
            {
                "type": "text",
                "content": "Ega va uning ifodalanishi.\n"
            }
        ]
    },
    {
        "id": 121,
        "question": [
            {
                "type": "text",
                "content": "O’zbek tili va uning taraqqiyot bosqichlari.\n"
            }
        ]
    },
    {
        "id": 122,
        "question": [
            {
                "type": "text",
                "content": "Imlo qoidalari nima?\n"
            }
        ]
    },
    {
        "id": 123,
        "question": [
            {
                "type": "text",
                "content": "Leksikologiya va uning obyekti.\n"
            }
        ]
    },
    {
        "id": 124,
        "question": [
            {
                "type": "text",
                "content": "Sifat so’z turkumi.\n"
            }
        ]
    },
    {
        "id": 125,
        "question": [
            {
                "type": "text",
                "content": "Kesim va uning ifodalanishi.\n"
            }
        ]
    },
    {
        "id": 126,
        "question": [
            {
                "type": "text",
                "content": "Undosh tovushlar tasnifi.\n"
            }
        ]
    },
    {
        "id": 127,
        "question": [
            {
                "type": "text",
                "content": "O’z va ko’chma ma’noli so’zlar.\n"
            }
        ]
    },
    {
        "id": 128,
        "question": [
            {
                "type": "text",
                "content": "O’zbek tili leksikasining rivojlanishi.\n"
            }
        ]
    },
    {
        "id": 129,
        "question": [
            {
                "type": "text",
                "content": "Otning ko’plik kategoriyasi.\n"
            }
        ]
    },
    {
        "id": 130,
        "question": [
            {
                "type": "text",
                "content": "Aniqlovchi va uning ifodalanishi.\n"
            }
        ]
    },
    {
        "id": 131,
        "question": [
            {
                "type": "text",
                "content": "Fonetika bo’limi haqida ma’lumot.\n"
            }
        ]
    },
    {
        "id": 132,
        "question": [
            {
                "type": "text",
                "content": "Adabiy til nima? Adabiy til normalariga nimalar kiradi.\n"
            }
        ]
    },
    {
        "id": 133,
        "question": [
            {
                "type": "text",
                "content": "Sinonim, Omonim, Antonim hodisasi.\n"
            }
        ]
    },
    {
        "id": 134,
        "question": [
            {
                "type": "text",
                "content": "Olmosh so’z turkumi.\n"
            }
        ]
    },
    {
        "id": 135,
        "question": [
            {
                "type": "text",
                "content": "To’ldiruvchi va uning ifodalanishi.\n"
            }
        ]
    },
    {
        "id": 136,
        "question": [
            {
                "type": "text",
                "content": "Sifat darajalari.\n"
            }
        ]
    },
    {
        "id": 137,
        "question": [
            {
                "type": "text",
                "content": "Bo’g’in va uning turlari haqida ma’lumot bering\n"
            }
        ]
    },
    {
        "id": 138,
        "question": [
            {
                "type": "text",
                "content": "So’z va uning ma’nosi: ko’p ma’noli so’zlar\n"
            }
        ]
    },
    {
        "id": 139,
        "question": [
            {
                "type": "text",
                "content": "Son so’z turkumi.\n"
            }
        ]
    },
    {
        "id": 140,
        "question": [
            {
                "type": "text",
                "content": "Hol va uning ifodalanishi.\n"
            }
        ]
    },
    {
        "id": 141,
        "question": [
            {
                "type": "text",
                "content": "Sonning ma’no turlari.\n"
            }
        ]
    },
    {
        "id": 142,
        "question": [
            {
                "type": "text",
                "content": "Sheva, lahja va adabiy tilni farqlang.\n"
            }
        ]
    },
    {
        "id": 143,
        "question": [
            {
                "type": "text",
                "content": "So’z yasovchi, so’z shakli yasovchi qo’shimchalar.\n"
            }
        ]
    },
    {
        "id": 144,
        "question": [
            {
                "type": "text",
                "content": "Otning modal kategoriyasi.\n"
            }
        ]
    },
    {
        "id": 145,
        "question": [
            {
                "type": "text",
                "content": "Gap va uning tuzilish jihatdan turlari.\n"
            }
        ]
    },
    {
        "id": 146,
        "question": [
            {
                "type": "text",
                "content": "Fe’l so’z turkumi.\n"
            }
        ]
    },
    {
        "id": 147,
        "question": [
            {
                "type": "text",
                "content": "Urg’u haqida ma’lumot\n"
            }
        ]
    },
    {
        "id": 148,
        "question": [
            {
                "type": "text",
                "content": "Morfemika (so’z tarkibi) bo’limi.\n"
            }
        ]
    },
    {
        "id": 149,
        "question": [
            {
                "type": "text",
                "content": "Fe’lning zamon kategoriyasi\n"
            }
        ]
    },
    {
        "id": 150,
        "question": [
            {
                "type": "text",
                "content": "Gapning ifoda maqsadiga ko’ra turlari\n"
            }
        ]
    },
    {
        "id": 151,
        "question": [
            {
                "type": "text",
                "content": "Olmosh va uning ma’no turlari.\n"
            }
        ]
    },
    {
        "id": 152,
        "question": [
            {
                "type": "text",
                "content": "Morfemika (so’z tarkibi) bo’limida nimalar o’rganiladi.\n"
            }
        ]
    },
    {
        "id": 153,
        "question": [
            {
                "type": "text",
                "content": "Frazeologiya bo’limi va uning turlari\n"
            }
        ]
    },
    {
        "id": 154,
        "question": [
            {
                "type": "text",
                "content": "Tarixiylik jihatdan o’zbek tili leksikasi.\n"
            }
        ]
    },
    {
        "id": 155,
        "question": [
            {
                "type": "text",
                "content": "O’zbek tilida so’z yasash usullari va uning turlari\n"
            }
        ]
    },
    {
        "id": 156,
        "question": [
            {
                "type": "text",
                "content": "Sifat va Sonning otlashish hodisasi\n"
            }
        ]
    },
    {
        "id": 157,
        "question": [
            {
                "type": "text",
                "content": "Fe’lning nisbat kategoriyasi\n"
            }
        ]
    },
    {
        "id": 158,
        "question": [
            {
                "type": "text",
                "content": "Fe’lning vazifadosh shakllari\n"
            }
        ]
    },
    {
        "id": 159,
        "question": [
            {
                "type": "text",
                "content": "Yordamchi so’z turkumlari\n"
            }
        ]
    },
    {
        "id": 160,
        "question": [
            {
                "type": "text",
                "content": "Alohida guruh so’zlari.\n"
            }
        ]
    },
    {
        "id": 161,
        "question": [
            {
                "type": "text",
                "content": "Sodda gap sintaksisi haqida ma’lumot\n"
            }
        ]
    },
    {
        "id": 162,
        "question": [
            {
                "type": "text",
                "content": "Qo’shma gap sintaksisi haqida ma’lumot\n"
            }
        ]
    },
    {
        "id": 163,
        "question": [
            {
                "type": "text",
                "content": "Bir ma’noli va ko’p ma’noli so’zlarning farqi\n"
            }
        ]
    },
    {
        "id": 164,
        "question": [
            {
                "type": "text",
                "content": "So’zning leksik va grammatik ma’nosi nima, izohlang.\n"
            }
        ]
    },
    {
        "id": 165,
        "question": [
            {
                "type": "text",
                "content": "Ma’no ko’chish yo’llari qaysilar.\n"
            }
        ]
    },
    {
        "id": 166,
        "question": [
            {
                "type": "text",
                "content": "Tinish belgilar nazariyasi va amaliyoti\n"
            }
        ]
    },
    {
        "id": 167,
        "question": [
            {
                "type": "text",
                "content": "Nutq madaniyatida nutq uslublari\n"
            }
        ]
    },
    {
        "id": 168,
        "question": [
            {
                "type": "text",
                "content": "So’z birikmalari tasnifi va qo’llanishi\n"
            }
        ]
    },
    {
        "id": 169,
        "question": [
            {
                "type": "text",
                "content": "O’zbek tilining asosiy imlo qoidalari haqida ma’lumot\n"
            }
        ]
    },
    {
        "id": 170,
        "question": [
            {
                "type": "text",
                "content": "Mustaqil so’z turkumlari\n"
            }
        ]
    }
]